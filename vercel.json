{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "DB_USER": "@db_user",
    "DB_PASS": "@db_pass",
    "DB_CLUSTER": "@db_cluster",
    "DB_NAME": "@db_name"
  }
}